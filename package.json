    "electron-packager": "^15.1.0",
    "electron-updater": "^4.3.5"
  },
  "build": {
    "appId": "com.balabit.helloworld",
    "win": {
      "category": "your.app.category.type",
      "target": [
        "win32"
      ]
    }
  }
